package com.npci.beans;

import java.time.LocalDate;

public class Profile {
	
	private int profile_id;
	private String name;
	private String emailId;
	private String password;
	private LocalDate dob;
	private long phone;
	private int wallet_id_ref;
	 
	public Profile() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Profile(int profile_id, String name, String emailId, String password, LocalDate dob, long phone,
			int wallet_id_ref) {
		super();
		this.profile_id = profile_id;
		this.name = name;
		this.emailId = emailId;
		this.password = password;
		this.dob = dob;
		this.phone = phone;
		this.wallet_id_ref = wallet_id_ref;
	}
	public int getProfile_id() {
		return profile_id;
	}
	public void setProfile_id(int profile_id) {
		this.profile_id = profile_id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEmailId() {
		return emailId;
	}
	public void setEmailId(String emailId) {
		this.emailId = emailId;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public LocalDate getDob() {
		return dob;
	}
	public void setDob(LocalDate dob) {
		this.dob = dob;
	}
	public long getPhone() {
		return phone;
	}
	public void setPhone(long phone) {
		this.phone = phone;
	}
	public int getWallet_id_ref() {
		return wallet_id_ref;
	}
	public void setWallet_id_ref(int wallet_id_ref) {
		this.wallet_id_ref = wallet_id_ref;
	}
	
	

}
